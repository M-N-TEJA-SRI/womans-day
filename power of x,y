    #include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int a[n],i,c=0;
		for(i=0;i<n;i++){
			cin>>a[i];
			if(a[i]<0)
			c++;
		}
		cout<<c<<endl;
	}
}
count of repeated words
#include <iostream>
#include<map>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int i,c=0;
		string s[n];
		map<string,int>mp;
		for(i=0;i<n;i++){
			
			cin>>s[i];
			mp[s[i]]++;
			
		}
		for(auto it:mp){
			if(it.second>=2)
			c++;
		}
		cout<<c<<endl;
	}
 }

    
