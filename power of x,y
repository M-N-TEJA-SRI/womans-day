#include <iostream>
#include<map>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int i,c=0;
		string s[n];
		map<string,int>mp;
		for(i=0;i<n;i++){
			
			cin>>s[i];
			mp[s[i]]++;
			
		}
		for(auto it:mp){
			if(it.second>=2)
			c++;
		}
		cout<<c<<endl;
	}
 }
 count of consonants
 t=int(input())
for i in range(t):
    s=input()
    c=0
    for i in range(len(s)):
        if(s[i] not in ['a','A','e','E','i','I','O','o','u','U']):
            c+=1
    print(c)        
    reverse
    #include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int rem,sum=0;
		while(n){
			rem=n%10;
			sum=sum*10+rem;
			n/=10;
		}
		cout<<sum<<endl;
	}
}
