    #include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int a[n],i,c=0;
		for(i=0;i<n;i++){
			cin>>a[i];
			if(a[i]<0)
			c++;
		}
		cout<<c<<endl;
	}
}
