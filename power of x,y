    sum of digits in arr
    #include <iostream>
#include<vector>
using namespace std;
int digit(int x){
	int rem,sum=0;
	while(x){
		rem=x%10;
		sum+=rem;
		x/=10;
	}
	return sum;
}
int main() {
	int t;
	cin>>t;
	while(t--){
		vector<int>v;
		int n;
		cin>>n;
		int a[n],i,ans;
		for(i=0;i<n;i++){
			cin>>a[i];
			ans=digit(a[i]);
			v.push_back(ans);
		}
		for(auto it:v){
			cout<<it<<" ";
		}
		cout<<endl;
	}
	
}
second star
#include <iostream>
#include<algorithm>
using namespace std;
int main() {
	
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int a[n],i;
		for(i=0;i<n;i++){
			cin>>a[i];
		}
		sort(a,a+n);
		cout<<a[n-2]<<endl;
	}
}
